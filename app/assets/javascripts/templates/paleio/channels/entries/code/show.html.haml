%td{ :style => 'vertical-align:top;width:35px;' }
  <%= model.hour() %>
%td{ :style => 'font-weight:bold;vertical-align:top;white-space:nowrap;text-align:right;' }
  <%= model.nick() %>
%td.input{ :style => 'vertical-align:top;padding-left:10px;width:100%;' }<
  %pre{ :style => '' }<
    %code{ :style => 'padding-left:20px;font-size:11px;border:1px solid #ddd;border-radius:3px;box-shadow: 0px 3px 4px #eee;' }<
      %span.label.label-info{ :style => 'font-family:Ubuntu,sans-serif;float:right;padding:0px 4px;font-weight:100;font-size:10px;' } <%= model.get('language') %>
      <%= model.prettyCode() %>